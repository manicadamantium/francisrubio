<a href="#main" class="skip-link">Skip to content</a>
<div class="navbar-wrapper" data-navbar-animations>
  <nav class="navigation-bar show-on-mobile" data-navbar-animations>
    <div class="navigation-bar__section">
      <button class="navigation-link has-icon--start" data-dialog="nav-menu">
        {% set feathericon = 'menu' %}
        {% include 'components/feathericon.njk' %}
        <span class="navigation-link-text">Menu</span>
      </button>
    </div>
    <div class="navigation-bar__section jc--center hide-on-small">
      <a href="/" style="text-decoration: none; " class="weight-300 fs-6">Francis Rubio</a>
    </div>
    <div class="navigation-bar__section jc--end">
      <div class="cluster gap">
        <button class="navigation-link has-icon--start" data-dialog="search-menu">
          {% set feathericon = 'search' %}
          {% include 'components/feathericon.njk' %}
          <span class="navigation-link-text">Search</span>
        </button>
        <button class="navigation-link" data-popover="form-color-scheme" data-aria-hide data-disclosure data-scroll-into-view="false">
          {% set feathericon = 'moon' %}
          {% include 'components/feathericon.njk' %}
          <span class="navigation-link-text sr-only">Change color Scheme</span>
        </button>
      </div>
    </div>
  </nav>
  <nav class="navigation-bar hide-on-mobile" data-navbar-animations>
    <div class="navigation-bar__section">
      <div class="navigation-bar__section jc--start">
        <ul class="navigation-list">
          <li class="navigation-list-item">
            <a href="/" class="navigation-link weight-700">Francis Rubio</a>
          </li>

          {% for item in navigation | rejectattr("overflow") %}
            <li class="navigation-list-item">
              <a href="{{ item.url }}" class="navigation-link {{ 'active' if utils.isActivePath(activePath, item.url) }}" style="--delay-factor: {{ loop.index0 }}">
                {% if item.text %}
                  <span class="navigation-link-text">{{ item.text }}</span>
                {% endif %}
                {% if item.feathericon %}
                  {% set feathericon = item.feathericon %}
                  {% include "components/feathericon.njk" %}
                {% endif %}
                {% if item.label %}
                  <span class="navigation-link-label">{{ item.label }}</span>
                {% endif %}
              </a>
            </li>
          {% endfor %}

          <li class="navigation-list-item" style="position: relative">
            <button class="navigation-link" data-popover data-aria-hide>
              {% set feathericon = 'more-horizontal' %}
              {% include 'components/feathericon.njk' %}
              <span class="sr-only">More items</span>
            </button>
            <div class="navigation-popup" hidden>
              <ul class="navigation-list">
                {% for item in navigation | selectattr("overflow") %}
                  <li class="navigation-list-item">
                    <a href="{{ item.url }}" class="navigation-link" style="--delay-factor: {{ loop.index0 }}">
                      {% if item.text %}
                        <span class="navigation-link-text">{{ item.text }}</span>
                      {% endif %}
                      {% if item.feathericon %}
                        {% set feathericon = item.feathericon %}
                        {% include "components/feathericon.njk" %}
                      {% endif %}
                      {% if item.label %}
                        <span class="navigation-link-label">{{ item.label }}</span>
                      {% endif %}
                    </a>
                  </li>
                {% endfor %}
              </ul>
            </div>
          </li>
        </ul>
      </div>
      <div class="navigation-bar__section jc--end">
        <div class="cluster gap">
          <button class="navigation-link has-icon--start" data-dialog="search-menu">
            {% set feathericon = 'search' %}
            {% include 'components/feathericon.njk' %}
            <span class="navigation-link-text">Search</span>
          </button>
          <button class="navigation-link" data-popover="form-color-scheme" data-aria-hide data-disclosure data-scroll-into-view="false">
            {% set feathericon = 'moon' %}
            {% include 'components/feathericon.njk' %}
            <span class="navigation-link-text sr-only">Change color Scheme</span>
          </button>
        </div>
      </div>
    </div>
  </nav>

  <form class="navigation-popup detached" data-navbar-popover-animations aria-label="Change color scheme" name="color-scheme" id="form-color-scheme" hidden>
    <div class="navigation-list gap-m">
      <div class="navigation-list-item navigation-list-radio">
        <input type="radio" id="desktop-colscheme-light" name="color-scheme" value="light">
        <label for="desktop-colscheme-light">Light</label>
      </div>
      <div class="navigation-list-item navigation-list-radio">
        <input type="radio" id="desktop-colscheme-dark" name="color-scheme" value="dark">
        <label for="desktop-colscheme-dark">Dark</label>
      </div>
      <div class="navigation-list-item navigation-list-radio">
        <input type="radio" id="desktop-colscheme-system" name="color-scheme" value="system">
        <label for="desktop-colscheme-system">Follow system theme</label>
      </div>
    </div>
  </form>
</div>

<div class="navigation-menu" id="nav-menu" aria-label="Navigation menu dialog" hidden>
  <div class="navigation-menu-backdrop" data-dialog-hide aria-hidden="true"></div>
  <nav class="navigation-menu-content" aria-label="Navigation menu">
    <div class="spaced cluster ai--center padding-inline--xxl padding-block--l">
      <a href="/" class="weight-300 font-sans navigation-menu-link">Francis Rubio</a>
      <button class="navigation-link has-icon--start" data-dialog-hide>
        {% set feathericon = 'x' %}
        {% include 'components/feathericon.njk' %}
        <span class="navigation-link-text">Close menu</span>
      </button>
    </div>
    <div class="navigation-menu-items">
      {% for item in navigation %}
        <a href="{{ item.url }}" class="navigation-menu-link" style="--delay-factor: {{ loop.index0 / 2 }}">
          {% if item.text %}
            <span class="navigation-menu-link-text">{{ item.text }}</span>
          {% endif %}
          {% if item.feathericon %}
            {% set feathericon = item.feathericon %}
            {% include "components/feathericon.njk" %}
          {% endif %}
          {% if item.label %}
            <span class="navigation-menu-link-label">{{ item.label }}</span>
          {% endif %}
        </a>
      {% endfor %}
    </div>
    <div class="navigation-menu-contact-items"></div>
  </nav>
</div>
<div class="modal" id="search-menu" aria-label="Search control" hidden>
  <div class="modal__backdrop" data-dialog-hide></div>
  <div class="search-control">
    <form name="search" aria-labelledby="search-label" class="search-container search-form">
      <div class="search-form-control">
        <input type="text" name="term" id="term" placeholder=" " />
        <label id="search-label" for="term">Search the site</label>
    </div>
    <button class="primary button padding--m">
      {% set feathericon = 'search' %}
      {% include "components/feathericon.njk" %}
      <span class="sr-only">Hanapin</span>
    </button>
  </form>
  <p class="results-count" data-search="count"></p>
  <ul class="results-container" id="search-results-list" aria-label="Search results"></ul>
  <template id="search-result-listitem-template">
    <li>
      <a class="result-item" href="#!">
        <img data-result="image" class="result-image" alt="">
        <span data-result="title" class="result-title"></span>
        <span data-result="subtitle" class="result-subtitle"></span>
        <time data-result="date" class="result-date" datetime="2021-04-11"></time>
      </a>
    </li>
  </template>
</div>
</div>
<div data-observer-trigger></div>