---
eleventyExcludeFromCollections: true
layout: null
permalink: /videos.xml
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Creative literature by Francis Rubio</title>
  <subtitle>My attempts in encapsulating my thoughts into written art</subtitle>
  <link href="{{ site.url }}/videos.xml" rel="self"/>
  <updated>{{ generated }}</updated>
  <author>
    <name>{{ authors.teacherbuknoy.name }}</name>
    <email>{{ authors.teacherbuknoy.email }}</email>
    <uri>{{ authors.teacherbuknoy.url }}</uri>
  </author>
  <id>{{ site.domain }}</id>
  <category term="videos"/>
  <icon>/assets/root/favicon.svg</icon>
  <rights>â“’ {{ utils.currentYear }} Francis Rubio</rights>

  {% for post in collections.videos %}
    {% set absolutePostUrl %}{{ site.url }}{{ post.url }}{% endset %}
    <entry>
      <title type="html">{{ post.data.title }}</title>
      <link href="https://youtu.be/{{ post.data.code }}"/>
      <updated>{{ post.date | dateToRfc3339 }}</updated>
      <id>https://youtu.be/{{ post.data.code }}</id>
      {% set person = authors[post.data.author] %}
      <author>
        <name>{{ person.name }}</name>
        {% if person.url %}
          <uri>{{ person.url }}</uri>
        {% endif %}
        {% if person.email %}
          <email>{{ person.email }}</email>
        {% endif %}
      </author>
      {% if post.data.tags and post.data.tags.length > 0 %}
        {% for tag in post.data.tags %}
          <category term="{{ tag }}"/>
        {% endfor %}
      {% endif %}

      {% if post.data.contributors and post.data.contributors.length > 0 %}
        {% for contributor in post.data.contributors %}
          {% set person = authors[contributor] %}
          <contributor>
            <name>{{ person.name }}</name>
            {% if person.url %}
              <uri>{{ person.url }}</uri>
            {% endif %}
            {% if person.email %}
              <email>{{ person.email }}</email>
            {% endif %}
          </contributor>
        {% endfor %}
      {% endif %}
      <content type="html">{{ post.templateContent | htmlToAbsoluteUrls(absolutePostUrl) }}</content>
    </entry>
  {% endfor %}
</feed>